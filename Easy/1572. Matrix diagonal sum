class Solution {
    public int diagonalSum(int[][] mat) {
        int m=mat.length;
        int n=mat[0].length;
        int sum=0;
        int len=mat[0].length;
        for(int i=0;i<n;i++){
            len--;
            for(int j=0;j<n;j++){
                if(i==j || j==len){
                    sum=sum+mat[i][j];
                }
            }
        }
        return sum;
    }
}
